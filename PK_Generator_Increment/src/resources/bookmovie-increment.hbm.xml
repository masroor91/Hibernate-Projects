<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="beans.BookMovie" table="TicketBooking" schema="Mas">
		<id name="bid" column="id">
			<generator class="increment"></generator>
		</id>
		<property name="movie" column="movie"/>
		<property name="showTime" column="showtime"/>
		<property name="seatno" column="seatno"/>
	
	</class>

</hibernate-mapping>